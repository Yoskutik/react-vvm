{
  "name": "@yoskutik/react-vvm",
  "version": "0.1.17",
  "description": "An MVVM implementation for React using MobX",
  "keywords": [
    "typescript",
    "react",
    "mobx",
    "mvvm",
    "architecture",
    "lightweight"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "pub": "yarn build && jest && yarn publish --access public",
    "lint": "eslint ./src --ext .ts,.tsx --quiet"
  },
  "peerDependencies": {
    "mobx": "^6 || ^5",
    "mobx-react": "^7 || ^6",
    "react": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.3",
    "@rollup/plugin-typescript": "8.5.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.1.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-is": "^17.0.3",
    "@yoskutik/eslint-config": "0.1.3",
    "eslint": "8.24.0",
    "jest": "29.1.2",
    "jest-babel": "^1.0.1",
    "jest-environment-jsdom": "^29.1.2",
    "mobx": "6.6.2",
    "mobx-react": "7.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "29.0.3",
    "tslib": "2.4.0",
    "tsyringe": "4.7.0",
    "typescript": "4.7.4"
  },
  "author": "Yoskutik <yoskutik@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "@yoskutik/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.spec.json"
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  }
}
