import React, { FC } from 'react';
import { childView } from '@yoskutik/react-vvm';
import type { SomeViewModel } from './path-to-view-model';

export type SomeChildViewProps = {
  prop1: number;
  prop2?: string;
};

export const SomeChildView: FC<SomeChildViewProps> = childView<SomeViewModel>()(({ viewModel, prop1, prop2 }) => (
  <div>
    {viewModel.field1}
    {prop1}
    {prop2}
  </div>
));
